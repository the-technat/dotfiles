#!/usr/bin/env zunit

@test 'homebrew is installed' {
 if [[ $OSTYPE =~ 'linux.*' ]]; then
    skip 'Homebrew is not installed on linux'
  fi
  run brew --version

  assert "$state" equals 0
  assert "$output" is_not_empty
}

@test 'mise is installed' {
  run mise --version

  assert "$state" equals 0
  assert "$output" is_not_empty
}